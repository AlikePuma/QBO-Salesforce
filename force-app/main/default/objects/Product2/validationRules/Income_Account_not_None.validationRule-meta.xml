<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Income_Account_not_None</fullName>
    <active>true</active>
    <errorConditionFormula>IF(
				Sell_Product_Service__c = true,
				ISPICKVAL(Income_Account__c, &apos;&apos;),
				false  
)</errorConditionFormula>
    <errorMessage>If Sell Product/Service is checked, you must specify an Income Account</errorMessage>
</ValidationRule>
